<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.info.yikao">


    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="com.huawei.android.launcher.permission.CHANGE_BADGE" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="cn.com.nbd.nbdmobile.permission.MIPUSH_RECEIVE" />
    <uses-permission android:name="android.permission.RECEIVE_USER_PRESENT" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />


    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:node="merge"
        android:usesCleartextTraffic="true"
        tools:replace="android:icon,android:theme">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.activity.UserOrderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.SchoolDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.CertifyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.SignUpListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.SignUpPayActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.SignUpDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.SignUpCertifyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.FastNewsListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.FastNewsDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.SchoolListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.ShowListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.ShowDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.MajorIntroActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ui.activity.CustomCaptureActivity"
            android:screenOrientation="sensorPortrait"
            android:theme="@style/CaptureTheme" />

    </application>

</manifest>